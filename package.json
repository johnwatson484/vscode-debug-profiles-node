{
  "name": "vscode-debug-profiles",
  "version": "1.0.0",
  "description": "Example debug profiles for VS Code",
  "main": "app/index.js",
  "scripts": {
    "start": "node app",
    "start:watch": "nodemon --inspect=0.0.0.0 app",
    "start:debug": "nodemon --inspect-brk=0.0.0.0 app",
    "test": "jest --runInBand",
    "test:lint": "eslint",
    "test:watch": "jest --coverage=false --onlyChanged --watch --runInBand",
    "test:debug": "node --inspect-brk=0.0.0.0 ./node_modules/jest/bin/jest.js --runInBand --no-cache --watchAll"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnwatson484/vscode-debug-profiles.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/johnwatson484/vscode-debug-profiles/issues"
  },
  "homepage": "https://github.com/johnwatson484/vscode-debug-profiles#readme",
  "dependencies": {
    "@hapi/hapi": "20.1.5",
    "nodemon": "3.1.4"
  },
  "devDependencies": {
    "eslint": "9.7.0",
    "jest": "27.2.0",
    "neostandard": "0.11.1"
  }
}
